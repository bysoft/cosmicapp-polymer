  "bugs": {
    "url": "https://github.com/Mandrewdarts/polymer-cosmicjs/issues"
  },
  "homepage": "https://github.com/Mandrewdarts/polymer-cosmicjs#readme",
  "dependencies": {
    "bower": "^1.7.7",
    "express": "^4.13.4",
    "npm": "^3.8.5"
  },
  "engines": {
    "node": "4.1.1"
  }
}
