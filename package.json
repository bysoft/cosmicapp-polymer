{
  "name": "polymer-cosmicjs",
  "version": "1.0.0",
  "description": "Creating blog homepage using [Polymer](https://www.polymer-project.org/1.0/) & [CosmicJS](https://cosmicjs.com/).",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "start": "npm run replace-vars; node server.js",
    "replace-vars": "sed \"s/{{COSMIC_BUCKET}}/$COSMIC_BUCKET/g\" index-template.html > index.html"
  },
